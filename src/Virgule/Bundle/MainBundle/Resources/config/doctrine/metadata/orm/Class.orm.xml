<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Class" table="class">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dayOfWeek" type="boolean" column="day_of_week" nullable="false"/>
    <field name="startTime" type="time" column="start_time" nullable="false"/>
    <field name="endTime" type="time" column="end_time" nullable="false"/>
    <field name="alternateStartdate" type="date" column="alternate_startdate" nullable="true"/>
    <field name="alternateEnddate" type="date" column="alternate_enddate" nullable="true"/>
    <field name="fkLevelId" type="integer" column="fk_level_id" nullable="false"/>
    <field name="fkSemesterId" type="integer" column="fk_semester_id" nullable="false"/>
    <field name="fkTeacherId" type="integer" column="fk_teacher_id" nullable="false"/>
  </entity>
</doctrine-mapping>
