<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Cours" table="cours">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="idCours" type="integer" column="id_cours">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="jour" type="boolean" column="jour"/>
    <field name="heureDebut" type="time" column="heure_debut"/>
    <field name="heureFin" type="time" column="heure_fin"/>
    <many-to-many field="fkApprenant" target-entity="Apprenants" mapped-by="fkCours"/>
    <many-to-one field="fkFormateur" target-entity="Formateurs" orphan-removal="">
      <join-columns>
        <join-column name="fk_id_formateur" referenced-column-name="id_formateur"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="fkNiveau" target-entity="Niveaux" orphan-removal="">
      <join-columns>
        <join-column name="fk_id_niveau" referenced-column-name="id_niveau"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="fkSession" target-entity="Sessions" orphan-removal="">
      <join-columns>
        <join-column name="fk_id_session" referenced-column-name="id_session"/>
      </join-columns>
    </many-to-one>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
