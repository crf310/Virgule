<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Delegations" table="delegations">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="idDelegation" type="integer" column="id_delegation">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomDelegation" type="string" column="nom_delegation" length="100"/>
    <field name="adresseDelegation" type="string" column="adresse_delegation" length="250"/>
    <field name="nomPresidentDelegation" type="string" column="nom_president_delegation" length="100"/>
    <field name="telephoneDelegation" type="string" column="telephone_delegation" length="10"/>
    <field name="adresseEmailDelegation" type="string" column="adresse_email_delegation" length="45"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
