<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RaisonsApprentissage" table="raisons_apprentissage">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="idRaisonApprentissage" type="integer" column="id_raison_apprentissage">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="libelleRaisonApprentissage" type="string" column="libelle_raison_apprentissage" length="100"/>
    <many-to-many field="fkApprenant" target-entity="Apprenants" inversed-by="fkRaisonApprentissage">
      <join-table name="a_comme">
        <join-columns>
          <join-column name="fk_id_raison_apprentissage" referenced-column-name="id_raison_apprentissage"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="fk_id_apprenant" referenced-column-name="id_apprenant"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
