<h1>Attachment</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Filename</th>
            <td>{{ entity.fileName }}</td>
        </tr>
        <tr>
            <th>Uploaddate</th>
            <td>{{ entity.UploadDate|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('attachment') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('attachment_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('attachment_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
