<h1>ClassSession list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Date</th>
            <th>Summary</th>
            <th>Fkclassid</th>
            <th>Fksessionteacherid</th>
            <th>Fksummaryteacherid</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('classsession_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{% if entity.date %}{{ entity.date|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.summary }}</td>
            <td>{{ entity.fkClassId }}</td>
            <td>{{ entity.fkSessionTeacherId }}</td>
            <td>{{ entity.fkSummaryTeacherId }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('classsession_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('classsession_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('classsession_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
