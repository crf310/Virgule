{% extends '::base.html.twig' %}
{% block body %}
<div id="errorMsg">
{{ form_errors(form) }}
</div>
<h1>Nouveau compte-rendu</h1>
Cours: {{ course }}
<form action="{{ path('classsession_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('classsession_index') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}
