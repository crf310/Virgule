<h1>OrganizationBranch</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Address</th>
            <td>{{ entity.address }}</td>
        </tr>
        <tr>
            <th>Presidentname</th>
            <td>{{ entity.presidentName }}</td>
        </tr>
        <tr>
            <th>Phonenumber</th>
            <td>{{ entity.phoneNumber }}</td>
        </tr>
        <tr>
            <th>Faxnumber</th>
            <td>{{ entity.faxNumber }}</td>
        </tr>
        <tr>
            <th>Emailaddress</th>
            <td>{{ entity.emailAddress }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('organizationbranch') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('organizationbranch_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('organizationbranch_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
